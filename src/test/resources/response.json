{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "count",
    "next",
    "previous",
    "results"
  ],
  "properties": {
    "count": {
      "$id": "#/properties/count",
      "type": "integer",
      "title": "The Count Schema",
      "default": 0,
      "examples": [
        61
      ]
    },
    "next": {
      "$id": "#/properties/next",
      "type": "string",
      "title": "The Next Schema",
      "default": "",
      "examples": [
        "https://swapi.co/api/planets/?page=2"
      ],
      "pattern": "^(.*)$"
    },
    "previous": {
      "$id": "#/properties/previous",
      "type": "null",
      "title": "The Previous Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "results": {
      "$id": "#/properties/results",
      "type": "array",
      "title": "The Results Schema",
      "items": {
        "$id": "#/properties/results/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "name",
          "rotation_period",
          "orbital_period",
          "diameter",
          "climate",
          "gravity",
          "terrain",
          "surface_water",
          "population",
          "residents",
          "films",
          "created",
          "edited",
          "url"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/results/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Alderaan"
            ],
            "pattern": "^(.*)$"
          },
          "rotation_period": {
            "$id": "#/properties/results/items/properties/rotation_period",
            "type": "string",
            "title": "The Rotation_period Schema",
            "default": "",
            "examples": [
              "24"
            ],
            "pattern": "^(.*)$"
          },
          "orbital_period": {
            "$id": "#/properties/results/items/properties/orbital_period",
            "type": "string",
            "title": "The Orbital_period Schema",
            "default": "",
            "examples": [
              "364"
            ],
            "pattern": "^(.*)$"
          },
          "diameter": {
            "$id": "#/properties/results/items/properties/diameter",
            "type": "string",
            "title": "The Diameter Schema",
            "default": "",
            "examples": [
              "12500"
            ],
            "pattern": "^(.*)$"
          },
          "climate": {
            "$id": "#/properties/results/items/properties/climate",
            "type": "string",
            "title": "The Climate Schema",
            "default": "",
            "examples": [
              "temperate"
            ],
            "pattern": "^(.*)$"
          },
          "gravity": {
            "$id": "#/properties/results/items/properties/gravity",
            "type": "string",
            "title": "The Gravity Schema",
            "default": "",
            "examples": [
              "1 standard"
            ],
            "pattern": "^(.*)$"
          },
          "terrain": {
            "$id": "#/properties/results/items/properties/terrain",
            "type": "string",
            "title": "The Terrain Schema",
            "default": "",
            "examples": [
              "grasslands, mountains"
            ],
            "pattern": "^(.*)$"
          },
          "surface_water": {
            "$id": "#/properties/results/items/properties/surface_water",
            "type": "string",
            "title": "The Surface_water Schema",
            "default": "",
            "examples": [
              "40"
            ],
            "pattern": "^(.*)$"
          },
          "population": {
            "$id": "#/properties/results/items/properties/population",
            "type": "string",
            "title": "The Population Schema",
            "default": "",
            "examples": [
              "2000000000"
            ],
            "pattern": "^(.*)$"
          },
          "residents": {
            "$id": "#/properties/results/items/properties/residents",
            "type": "array",
            "title": "The Residents Schema",
            "items": {
              "$id": "#/properties/results/items/properties/residents/items",
              "type": "string",
              "title": "The Items Schema",
              "default": "",
              "examples": [
                "https://swapi.co/api/people/5/",
                "https://swapi.co/api/people/68/",
                "https://swapi.co/api/people/81/"
              ],
              "pattern": "^(.*)$"
            }
          },
          "films": {
            "$id": "#/properties/results/items/properties/films",
            "type": "array",
            "title": "The Films Schema",
            "items": {
              "$id": "#/properties/results/items/properties/films/items",
              "type": "string",
              "title": "The Items Schema",
              "default": "",
              "examples": [
                "https://swapi.co/api/films/6/",
                "https://swapi.co/api/films/1/"
              ],
              "pattern": "^(.*)$"
            }
          },
          "created": {
            "$id": "#/properties/results/items/properties/created",
            "type": "string",
            "title": "The Created Schema",
            "default": "",
            "examples": [
              "2014-12-10T11:35:48.479000Z"
            ],
            "pattern": "^(.*)$"
          },
          "edited": {
            "$id": "#/properties/results/items/properties/edited",
            "type": "string",
            "title": "The Edited Schema",
            "default": "",
            "examples": [
              "2014-12-20T20:58:18.420000Z"
            ],
            "pattern": "^(.*)$"
          },
          "url": {
            "$id": "#/properties/results/items/properties/url",
            "type": "string",
            "title": "The Url Schema",
            "default": "",
            "examples": [
              "https://swapi.co/api/planets/2/"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}